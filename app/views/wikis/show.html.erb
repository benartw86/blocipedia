
  <div class="jumbotron">
    <%= @wiki.title %>
  </div>
  
  <div class="row">
    <div class="col-md-8">
      <p><%= @wiki.body%></p>      
    </div>      
  </div>
  </br>
  <div class="col-md-4">
    <%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn-lg btn-success' %>          
    <%= link_to "Delete Wiki", @wiki, method: :delete, class: 'btn-lg btn-danger', data: {confirm: 'Are you sure you want to delte this wiki forever?'} %>
  </div>
